{
  "name": "brookjs",
  "version": "0.8.6",
  "description": "A framework for building streaming applications.",
  "main": "cjs/index.js",
  "browser": "dist/brook.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "cjs",
    "es",
    "src"
  ],
  "scripts": {
    "test": "npm run test:lint && npm run test:unit && if test \"$SAUCE_USERNAME\" != ''; then npm run test:ci; fi",
    "test:lint": "eslint src --cache true --fix",
    "test:unit": "cross-env NODE_ENV=test karma start karma.conf.js --single-run",
    "test:ci": "cross-env NODE_ENV=test:ci karma start karma.conf.ci.js --single-run",
    "dev": "cross-env NODE_ENV=test karma start karma.conf.js",
    "clean": "rimraf cjs dist es coverage",
    "build:commonjs": "cross-env NODE_ENV=commonjs babel src --ignore __tests__ --out-dir cjs",
    "build:es": "cross-env NODE_ENV=es babel src --ignore __tests__ --out-dir es",
    "build:umd": "cross-env NODE_ENV=es webpack src/index.js dist/brook.js",
    "build:umd:min": "cross-env NODE_ENV=es webpack src/index.js dist/brook.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "prepublishOnly": "npm run clean && npm test && npm run build"
  },
  "authors": [
    "James DiGioia <jamesorodig@gmail.com> (http://jamesdigioia.com)",
    "Valtech (https://www.valtech.com/)"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.13.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-ramda": "^1.1.3",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-register": "^6.9.0",
    "chai": "^4.0.2",
    "chai-dom": "^1.4.0",
    "chai-spies": "^0.7.1",
    "core-js": "^2.4.1",
    "cross-env": "^5.0.1",
    "diffhtml-render-to-string": "^1.0.0-beta.8",
    "eslint": "^4.0.0",
    "eslint-config-valtech": "^1.0.0",
    "handlebars": "^4.0.6",
    "handlebars-loader": "^1.4.0",
    "handlebars-spec": "^3.0.3-alpha.1",
    "json-loader": "^0.5.4",
    "karma": "^1.4.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-sauce-launcher": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.1",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "simulant": "^0.2.2",
    "sinon": "^3.0.0",
    "sinon-chai": "^2.8.0",
    "webpack": "^3.0.0",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "diffhtml": "1.0.0-beta.8",
    "htmlparser2": "^3.9.2",
    "kefir": "^3.3.0",
    "ramda": "^0.24.1",
    "redux": "^3.5.2"
  }
}
