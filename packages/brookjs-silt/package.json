{
  "name": "brookjs-silt",
  "version": "0.11.13",
  "description": "Handlebars-based rendering engine, built on diffhtml.",
  "main": "cjs/index.js",
  "scripts": {
    "test": "npm run test:lint && npm run test:type && npm run test:unit",
    "test:lint": "eslint src --cache true --fix",
    "test:type": "flow check",
    "test:unit": "beaver test unit --coverage true --env test",
    "test:int": "cross-env NODE_ENV=test mocha --require babel-register int/run.js",
    "dev": "beaver dev tdd --env test",
    "clean": "rimraf cjs dist es coverage",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "build:commonjs": "cross-env NODE_ENV=commonjs babel src --ignore __tests__ --out-dir cjs",
    "build:es": "cross-env NODE_ENV=es babel src --ignore __tests__ --out-dir es",
    "build:umd": "cross-env NODE_ENV=es webpack src/index.js dist/brook.js",
    "build:umd:min": "cross-env NODE_ENV=es webpack src/index.js dist/brook.min.js",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
  "author": "James DiGioia <jamesorodig@gmail.com> (http://jamesdigioia.com)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-brookjs": "^0.11.3",
    "babel-register": "^6.26.0",
    "brookjs-cli": "^0.11.11",
    "brookjs-desalinate": "^0.11.11",
    "chai": "^4.1.2",
    "cross-env": "^5.0.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.7.0",
    "eslint-config-valtech": "^1.1.0",
    "eslint-import-resolver-node": "^0.3.1",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.5.1",
    "flow-bin": "^0.75.0",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "raf": "^3.4.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2",
    "sinon": "^5.1.0",
    "sinon-chai": "^3.1.0",
    "webpack": "^3.6.0",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "brookjs": "^0.11.11",
    "create-react-context": "^0.2.1",
    "kefir": "^3.8.1",
    "prop-types": "^15.6.0",
    "ramda": "^0.25.0",
    "react": "^16.3.0"
  }
}
