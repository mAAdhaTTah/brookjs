{
  "name": "brookjs-silt",
  "version": "0.10.0",
  "description": "Handlebars-based rendering engine, built on diffhtml.",
  "main": "cjs/index.js",
  "private": true,
  "scripts": {
    "test": "npm run test:lint && npm run test:type && npm run test:unit && npm run test:int",
    "test:lint": "eslint src --cache true --fix",
    "test:type": "flow check",
    "test:unit": "cross-env NODE_ENV=test nyc mocha --opts mocha.opts",
    "test:int": "cross-env NODE_ENV=test mocha --require babel-register int/run.js",
    "dev": "cross-env NODE_ENV=test karma start karma.conf.js",
    "clean": "rimraf cjs dist es coverage",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "build:commonjs": "cross-env NODE_ENV=commonjs babel src --ignore __tests__ --out-dir cjs",
    "build:es": "cross-env NODE_ENV=es babel src --ignore __tests__ --out-dir es",
    "build:umd": "cross-env NODE_ENV=es webpack src/index.js dist/brook.js",
    "build:umd:min": "cross-env NODE_ENV=es webpack src/index.js dist/brook.min.js",
    "prepublishOnly": "npm run clean && npm test && npm run build"
  },
  "author": "James DiGioia <jamesorodig@gmail.com> (http://jamesdigioia.com)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-ramda": "^1.4.3",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.0.5",
    "diffhtml-render-to-string": "^1.0.0-beta.8",
    "eslint": "^4.7.0",
    "eslint-config-valtech": "^1.1.0",
    "eslint-import-resolver-node": "^0.3.1",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "flow-bin": "^0.54.1",
    "glob": "^7.1.2",
    "handlebars": "^4.0.10",
    "handlebars-spec": "^3.0.3-alpha.1",
    "kefir": "^3.7.4",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "rimraf": "^2.6.2",
    "webpack": "^3.6.0",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "diffhtml": "^1.0.0-beta.8",
    "htmlparser2": "^3.9.2",
    "ramda": "^0.24.1"
  }
}
