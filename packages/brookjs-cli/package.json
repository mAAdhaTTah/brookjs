{
  "name": "brookjs-cli",
  "version": "1.0.0-alpha.10",
  "description": "command line tool for building brookjs applications",
  "main": "dist/brookjs-cli.js",
  "bin": {
    "beaver": "./bin/beaver.js"
  },
  "files": [
    "bin",
    "dist",
    "templates"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc --emitDeclarationOnly && rollup -c ../../rollup.config.ts",
    "clean": "rimraf dist coverage",
    "test": "npm run test:lint && npm run test:unit && npm run test:feature",
    "test:lint": "prettier --check \"src/**/*.{ts,tsx}\" && eslint \"src/**/*.{ts,tsx}\"",
    "test:unit": "beaver test unit --coverage",
    "test:feature": "cross-env NODE_ENV=test cucumber-js  features/**/*.feature --require '../../setupTests.ts' --require 'features/support/*.ts'",
    "dev": "beaver dev tdd --env test"
  },
  "author": "James DiGioia <jamesorodig@gmail.com> (https://jamesdigioia.com)",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.0.0",
    "babel-loader": "^8.0.0",
    "brookjs": "^1.0.0-alpha.10",
    "caporal": "^0.10.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "^2.1.0",
    "change-case": "^3.0.1",
    "cosmiconfig": "^5.0.7",
    "cross-env": "^5.1.1",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "esm": "^3.2.20",
    "execa": "^1.0.0",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.2",
    "handlebars": "^4.0.10",
    "handlebars-loader": "^1.6.0",
    "has-ansi": "^3.0.0",
    "hygen": "^4.0.2",
    "ink": "^2.1.0",
    "ink-select-input": "^3.1.0",
    "ink-spinner": "^3.0.0",
    "ink-text-input": "^3.0.0",
    "inquirer": "^6.2.2",
    "io-ts": "^1.6.4",
    "kefir": "^3.8.0",
    "lerna": "^3.0.6",
    "mocha": "^5.0.5",
    "nyc": "^12.0.2",
    "ramda": "^0.25.0",
    "react": "^16.8.1",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.3",
    "shelljs": "^0.8.0",
    "typesafe-actions": "^3.2.1",
    "typescript-nullable": "^0.6.0",
    "webpack": "^4.0.0",
    "webpack-plugin-install-deps": "^4.0.5",
    "yargs": "^12.0.5"
  },
  "gitHead": "b70ddce5231a58a1568bc86737756b42a73a7686"
}
