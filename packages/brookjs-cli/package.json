{
  "name": "brookjs-cli",
  "version": "1.0.0-beta.19",
  "description": "command line tool for building brookjs applications",
  "main": "dist/brookjs-cli.js",
  "types": "dist/index.d.ts",
  "bin": {
    "beaver": "./bin/beaver.js"
  },
  "files": [
    "bin",
    "dist",
    "jest",
    "templates"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc --emitDeclarationOnly && rollup -c ../../rollup.config.ts",
    "clean": "rimraf dist coverage *.tgz",
    "test": "beaver test check && beaver test lint && beaver test unit --coverage",
    "test:feature": "cross-env NODE_ENV=test cucumber-js  features/**/*.feature --require './features/setupTests.ts' --require 'features/support/*.ts'",
    "format": "beaver format"
  },
  "author": "James DiGioia <jamesorodig@gmail.com> (https://jamesdigioia.com)",
  "license": "MIT",
  "dependencies": {
