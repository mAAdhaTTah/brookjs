{
  "name": "brookjs-cli",
  "version": "1.0.0-alpha.25",
  "description": "command line tool for building brookjs applications",
  "main": "dist/brookjs-cli.js",
  "types": "dist/index.d.ts",
  "bin": {
    "beaver": "./bin/beaver.js"
  },
  "files": [
    "bin",
    "dist",
    "jest",
    "templates"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc --emitDeclarationOnly && rollup -c ../../rollup.config.ts",
    "clean": "rimraf dist coverage",
    "test": "yarn test:lint && yarn test:unit && yarn test:feature",
    "test:lint": "prettier --check \"src/**/*.{js,ts,tsx}\" && eslint \"src/**/*.{ts,tsx}\"",
    "test:unit": "beaver test unit --coverage",
    "test:feature": "cross-env NODE_ENV=test cucumber-js  features/**/*.feature --require './features/setupTests.ts' --require 'features/support/*.ts'",
    "dev": "beaver dev tdd",
    "format": "prettier --write \"src/**/*.{js,ts,tsx}\""
  },
  "author": "James DiGioia <jamesorodig@gmail.com> (https://jamesdigioia.com)",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.0",
    "brookjs": "^1.0.0-alpha.25",
    "camelcase": "^5.3.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "^2.1.0",
    "change-case": "^3.0.1",
    "cosmiconfig": "^5.0.7",
    "cross-env": "^6.0.0",
    "eslint": "^5.0.1",
    "eslint-loader": "^3.0.0",
    "esm": "^3.2.20",
    "execa": "^1.0.0",
    "fs-extra": "^8.0.1",
    "glob": "^7.1.2",
    "has-ansi": "^4.0.0",
    "hygen": "^5.0.1",
    "identity-obj-proxy": "^3.0.0",
    "ink": "^2.1.0",
    "ink-select-input": "^3.1.0",
    "ink-spinner": "^3.0.0",
    "ink-text-input": "^3.0.0",
    "inquirer": "^7.0.0",
    "io-ts": "^1.6.4",
    "jest": "^24.0.0",
    "jest-environment-jsdom-fourteen": "^0.1.0",
    "kefir": "^3.8.0",
    "react": "^16.8.1",
    "react-app-polyfill": "^1.0.4",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.3",
    "typesafe-actions": "^5.1.0",
    "webpack": "^4.0.0",
    "webpack-plugin-install-deps": "^4.0.5",
    "yargs": "^12.0.5"
  },
  "gitHead": "b70ddce5231a58a1568bc86737756b42a73a7686"
}
