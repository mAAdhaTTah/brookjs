{
  "name": "brookjs-cli",
  "version": "0.11.16",
  "description": "A command line interface for building brookjs applications.",
  "main": "src/index.js",
  "bin": {
    "beaver": "./bin/beaver.js"
  },
  "scripts": {
    "test": "npm run test:lint && npm run test:unit && npm run test:feature",
    "test:lint": "eslint ./src --fix",
    "test:unit": "beaver test unit --coverage",
    "test:feature": "cucumber-js",
    "dev": "beaver dev tdd --env test",
    "prepublishOnly": "npm test"
  },
  "author": "James DiGioia <jamesorodig@gmail.com> (http://jamesdigioia.com)",
  "license": "MIT",
  "dependencies": {
    "babel-loader": "^7.1.2",
    "brookjs": "^0.11.16",
    "caporal": "^0.10.0",
    "chalk": "^2.1.0",
    "change-case": "^3.0.1",
    "cross-env": "^5.1.1",
    "eslint-loader": "^1.9.0",
    "esm": "^3.0.8",
    "fs-extra": "^6.0.0",
    "glob": "^7.1.2",
    "handlebars": "^4.0.10",
    "handlebars-loader": "^1.6.0",
    "inquirer": "^6.0.0",
    "kefir": "^3.8.0",
    "lerna": "^2.8.0",
    "mocha": "^5.0.5",
    "npm-install-webpack-plugin": "^4.0.5",
    "nyc": "^11.2.1",
    "ramda": "^0.25.0",
    "redux": "^3.7.2",
    "shelljs": "^0.8.0",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "webpack": "^3.10.0"
  },
  "esm": {
    "mode": "auto",
    "cjs": true
  }
}
