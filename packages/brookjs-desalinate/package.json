{
  "name": "brookjs-desalinate",
  "version": "1.0.0-alpha.21",
  "description": "testing tools for brookjs applications",
  "main": "dist/brookjs-desalinate.cjs.js",
  "module": "dist/brookjs-desalinate.esm.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "kefir",
    "browser",
    "chai",
    "chai-plugin",
    "jest",
    "brookjs",
    "observables"
  ],
  "files": [
    "register.js",
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "prettier --check \"{src,.storybook,stories}/**/*.{js,ts,tsx}\" && eslint \"src/**/*.{ts,tsx}\"",
    "test:unit": "beaver test unit --coverage --env test && cross-env NODE_ENV=test jest",
    "dev": "beaver dev tdd --env test",
    "clean": "rimraf dist coverage",
    "build": "tsc --emitDeclarationOnly || true && rollup -c ../../rollup.config.ts",
    "storybook": "start-storybook -p 6006",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "author": "James DiGioia <jamesorodig@gmail.com> (https://jamesdigioia.com)",
  "license": "MIT",
  "dependencies": {
    "@storybook/addons": "^5.0.0",
    "@storybook/components": "^5.0.0",
    "@storybook/core-events": "^5.0.0",
    "@storybook/react": "^5.0.0",
    "@storybook/theming": "^5.0.0",
    "brookjs-silt": "^1.0.0-alpha.21",
    "chai-kefir": "^2.0.4",
    "deep-eql": "^4.0.0",
    "jest-kefir": "^1.0.0",
    "kefir-test-utils": "^1.0.0",
    "lolex": "^4.0.1",
    "polished": "^3.2.0",
    "react": "^16.8.0",
    "react-inspector": "^2.3.1",
    "uuid": "^3.3.2"
  },
  "peerDependencies": {
    "chai": "^4.0.0",
    "jest": "^24.0.0",
    "kefir": "^3.8.0",
    "react-testing-library": "^7.0.0"
  },
  "gitHead": "b70ddce5231a58a1568bc86737756b42a73a7686"
}
