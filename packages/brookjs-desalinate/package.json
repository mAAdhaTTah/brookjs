    "brookjs-silt": "^1.0.0-beta.17",
    "jest-kefir": "^2.3.0",
    "kefir-test-utils": "^1.0.0",
    "polished": "^3.2.0",
    "react": "^16.8.0",
    "react-inspector": "^2.3.1",
    "uuid": "^3.3.2"
