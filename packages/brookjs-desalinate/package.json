{
  "name": "brookjs-desalinate",
  "version": "1.0.0-alpha.1",
  "description": "testing tools for brookjs applications",
  "main": "dist/brookjs-desalinate.cjs.js",
  "browser": "dist/brookjs-desalinate.umd.js",
  "module": "dist/brookjs-desalinate.esm.js",
  "keywords": [
    "kefir",
    "browser",
    "chai",
    "chai-plugin",
    "brookjs",
    "observables"
  ],
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint src --fix",
    "test:unit": "beaver test unit --coverage --env test",
    "dev": "beaver dev tdd --env test",
    "clean": "rimraf dist coverage",
    "build": "rollup -c ../../rollup.config.js",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "author": "James DiGioia <jamesorodig@gmail.com> (http://jamesdigioia.com)",
  "license": "MIT",
  "dependencies": {
    "chai-kefir": "^2.0.4",
    "deep-eql": "^3.0.1",
    "lolex": "^2.3.1"
  },
  "peerDependencies": {
    "chai": "^4.0.0",
    "kefir": "^3.8.0"
  },
  "gitHead": "b70ddce5231a58a1568bc86737756b42a73a7686"
}
