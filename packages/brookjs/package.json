{
  "name": "brookjs",
  "version": "0.11.16",
  "description": "reactive programming tools for well-structured, testable applications.",
  "main": "dist/brookjs.cjs.js",
  "browser": "dist/brookjs.umd.js",
  "module": "dist/brookjs.esm.js",
  "files": [
    "dist",
    "cjs",
    "es",
    "src"
  ],
  "scripts": {
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint src --fix",
    "test:unit": "beaver test unit --coverage --env test",
    "dev": "beaver dev tdd --env test",
    "clean": "rimraf dist coverage",
    "build": "rollup -c ../../rollup.config.js",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
  "authors": [
    "James DiGioia <jamesorodig@gmail.com> (http://jamesdigioia.com)",
    "Valtech (https://www.valtech.com/)"
  ],
  "license": "MIT",
  "peerDependencies": {
    "kefir": "^3.8.0"
  },
  "dependencies": {
    "ramda": "^0.25.0"
  }
}
