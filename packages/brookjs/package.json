{
  "name": "brookjs",
  "version": "0.11.10",
  "description": "A framework for building streaming applications.",
  "main": "cjs/index.js",
  "browser": "dist/brook.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "cjs",
    "es",
    "src"
  ],
  "scripts": {
    "test": "npm run test:lint && npm run test:type && npm run test:unit",
    "test:lint": "eslint src --cache true --fix",
    "test:type": "flow check",
    "test:unit": "npm run test:unit:node && npm run test:unit:karma && if test \"$SAUCE_USERNAME\" != ''; then npm run test:unit:ci; fi",
    "test:unit:node": "beaver test unit --coverage true --env test:node",
    "test:unit:karma": "cross-env NODE_ENV=test:karma karma start karma.conf.js --single-run",
    "test:unit:ci": "cross-env NODE_ENV=test:karma karma start karma.conf.ci.js --single-run",
    "dev": "cross-env NODE_ENV=test karma start karma.conf.js",
    "clean": "rimraf cjs dist es coverage",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "build:commonjs": "cross-env NODE_ENV=commonjs babel src --ignore __tests__ --out-dir cjs",
    "build:es": "cross-env NODE_ENV=es babel src --ignore __tests__ --out-dir es",
    "build:umd": "cross-env NODE_ENV=es webpack src/index.js dist/brook.js",
    "build:umd:min": "cross-env NODE_ENV=es webpack src/index.js dist/brook.min.js",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
  "authors": [
    "James DiGioia <jamesorodig@gmail.com> (http://jamesdigioia.com)",
    "Valtech (https://www.valtech.com/)"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-brookjs": "^0.11.3",
    "babel-register": "^6.9.0",
    "brookjs-cli": "^0.11.10",
    "brookjs-desalinate": "^0.11.10",
    "chai": "^4.1.2",
    "chai-dom": "^1.4.0",
    "chai-kefir": "^2.0.2",
    "core-js": "^2.4.1",
    "cross-env": "^5.0.1",
    "diffhtml-render-to-string": "^1.0.0-beta.8",
    "eslint": "^4.0.0",
    "eslint-config-valtech": "^1.0.0",
    "eslint-import-resolver-node": "^0.3.1",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "flow-bin": "^0.71.0",
    "handlebars": "^4.0.6",
    "handlebars-loader": "^1.4.0",
    "handlebars-spec": "^3.0.3-alpha.1",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
    "json-loader": "^0.5.4",
    "karma": "^2.0.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-sauce-launcher": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "nyc": "^11.2.1",
    "raf": "^3.4.0",
    "rimraf": "^2.5.4",
    "simulant": "^0.2.2",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.8.0",
    "symbol-observable": "^1.2.0",
    "webpack": "^3.0.0",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "diffhtml": "1.0.0-beta.9",
    "htmlparser2": "^3.9.2",
    "kefir": "^3.8.0",
    "ramda": "^0.25.0",
    "redux": "^3.5.2"
  }
}
