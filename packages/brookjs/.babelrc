{
  "plugins": [
    "ramda",
    ["transform-object-rest-spread", { "useBuiltIns": true}],
    "syntax-flow",
    "transform-flow-strip-types"
  ],
  "env": {
    "test:node": {
      "presets": [
        ["es2015", { "loose": false, "modules": "commonjs" }]
      ]
    },
    "test:karma": {
      "presets": [
        ["es2015", { "loose": false, "modules": false }]
      ]
    },
    "commonjs": {
      "presets": [
        ["es2015", { "loose": false, "modules": "commonjs" }]
      ]
    },
    "es": {
      "presets": [
        ["es2015", { "loose": false, "modules": false }]
      ]
    }
  }
}
